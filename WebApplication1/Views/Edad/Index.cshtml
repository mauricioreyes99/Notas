
@{
    ViewBag.Title = "Index";
    
}

<h2>Edad...</h2>

<div class="container">
<div class="row"></div>
<div class="col-sm-2"></div>
<div class="col-sm-8"></div>
<form action="/" class="form-horizontal" method="post" id="formulario">
    <div class="form-group">
        <input type="text" name="txtNombre" class=" form-control" value="" autocomplete="off" placeholder="Digite su Nombre." />
    </div>
    <div class="form-group">
        <input type="text" name="txtEdad" class=" form-control" value="" autocomplete="off" placeholder="Digite su año de nacimiento." />
    </div>
    <div class=" form-group">
        <button name="btn" class="btn btn-info btn-lg">Calcular</button>
    </div>
</form>
<div class="jumbotron">
    <p>Resultados</p>
    <h2>@ViewBag.Edad</h2>
</div>
<div class="col-sm-2"></div>
</div>